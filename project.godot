;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Walker",
"language": "GDScript",
"path": "res://Randomwalker_algorithm.gd"
} ]
_global_script_class_icons={
"Walker": ""
}

[application]

config/name="Glonkok"
run/main_scene="res://Splash_screen.tscn"
